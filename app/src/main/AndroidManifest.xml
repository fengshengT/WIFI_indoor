    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/location2"
        android:label="室内定位1.0"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewActivity"></activity>
        <activity android:name=".ButtonActivity"></activity>
        <activity android:name=".EditTextActivity"></activity>
        <activity android:name=".ResualtActivity"></activity>
        <activity android:name=".set_areaActivity"></activity>
        <activity android:name=".area_informationActivity"></activity>
        <activity android:name=".collect_fignerprintActivity"></activity>
        <activity android:name=".extend_fignerprintActivity"></activity>
        <activity android:name=".locationActivity"></activity>
        <activity android:name=".location_settingActivity"></activity>
    </application>

</manifest>